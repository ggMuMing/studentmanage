<!DOCTYPE html>
<html lang="zh-CN">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="css/bootstrap.min.css">
  <link rel="stylesheet" href="css/main.css">
  <title>Manage</title>
</head>


<body>
  <!-- 页面顶部导航栏开始 -->
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <!-- 页面缩小后的顶部导航栏 -->
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1"
          aria-expanded="false">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <!-- 公司标志图片 -->
        <a class="navbar-brand" href="#">
          <img src="images/favicon.jpg">
        </a>
      </div>

      <!-- 导航按钮 -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
          <li class="active" id="allClass">
            <a href="/">随机点名</a>
          </li>
          <li id="manageClass">
            <a href="/class">班级管理</a>
          </li>
      </div>
      <!-- 导航按钮结束 -->
    </div>
  </nav>
  <!-- 页面顶部导航栏结束 -->
  
  <!-- 主体部分 -->
  <div class="container">
    <!-- 顶部居中标题 -->
    <h1 class="text-center">班级选择</h1>

    <!-- 主体中间部分 -->
    <div class="row">
      <div class="col-md-12">
        <div class="panel panel-default">
          <div class="panel-heading">所有班级</div>
          <div class="panel-body">
            <div class="btn-group ctrlWidth">
              <button type="button" class="btn btn-danger btn-lg btn-block" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                请通知管理员数据库异常，或者添加数据库后再操作！！！
              </button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>



  <script type="text/javascript" src="js/jquery-3.2.1.min.js"></script>
  <script type="text/javascript" src="js/bootstrap.min.js"></script>
  <script type="text/javascript" src="js/main.js"></script>
</body>

</html>